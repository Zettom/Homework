
size_t my_strlen(char *src)
{
    int cnt = 0;
    while (src[cnt] != '\0')
    {
        cnt++;
    };
    return cnt;
}

int my_strcmp(char *s1, char *s2)
{
    int cnt = 0;
    while (s1[cnt] == s2[cnt])
    {
        if ((s1[cnt] == '\0') && (s1[cnt] == '\0' ))
            {
                return 0;
            }
        cnt++;
    };
    int result = s1[cnt]-s2[cnt];
    return result;
}

void my_strcpy(char *dst, char *src)
{
    int cnt = 0;
    do
    {
        dst[cnt] = src[cnt];
        cnt++;
    } while(src[cnt] != '\0');
}

void my_strctt (char *dst, char *src)
{
    int cnt = my_strlen(dst);
    int cnt2 = 0;
    do
    {
        dst[cnt] = src[cnt2];
        cnt++;
        cnt2++;
    } while(src[cnt2] != '\0');
}
